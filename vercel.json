{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/bot.py"
    }
  ],
  "cleanUrls": true,
  "regions": ["iad1"],
  "public": false,
  "github": {
    "autoAlias": true,
    "silent": true,
    "autoJobCancelation": true
  },
  "functions": {
    "api/bot.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}